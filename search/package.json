{
  "name": "@approvals/search",
  "private": true,
  "version": "2.2.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run import && npm run build && vite preview",
    "test": "vitest",
    "import": "npm run import:page && npm run import:service && npm run import:user",
    "import:user": "rm -rf node_modules/.vite && import ../../javascript-libraries/datimuser",
    "import:page": "rm -rf node_modules/.vite && import ../page",
    "import:service": "rm -rf node_modules/.vite && import ../service",
    "postinstall": "npm run import"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-data-grid": "^6.18.2",
    "@pepfar-react-lib/datimuser": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pepfar-react-lib/local-import": "1.0.8",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^12.10.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
